<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD//EN"  
 	"http://hibernate.org/dtd/hibernate-mapping.dtd">

<hibernate-mapping>
	
	<class name="sge.Entidades.Zona" table= "ZONAS">
		<id name="id" column="ID"><generator class="native"></generator></id>
		<property name="nombre" 		column="NOMBRE"></property>
		<property name="radio" 			column="RADIO"></property>
		<property name="latitud" 		column="LATITUD"></property>
		<property name="longitud" 		column="LONGITUD"></property>

	</class>
	
</hibernate-mapping>